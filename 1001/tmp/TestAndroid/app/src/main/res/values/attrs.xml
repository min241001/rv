        <attr name="month_view" format="string" />
        <attr name="week_view" format="string" />
        <attr name="year_view" format="string" />
        <attr name="week_bar_height" format="dimension" />
        <attr name="week_bar_view" format="string" />

        <attr name="scheme_text" format="string" />
        <attr name="day_text_size" format="dimension" />
        <attr name="lunar_text_size" format="dimension" />
        <attr name="calendar_height" format="dimension" />
